<% provide(:title, "新規投稿") %>

<h1>記事の新規投稿</h1>
<%= form_with model: @article, local: true do |f| %>
  <%= render partial: 'shared/error_messages', locals: { object: f.object } %>

  <%= f.label :title, "タイトル" %>
  <%= f.text_area :title,  rows: 2 %>

  <%= f.label :content, "内容" %>
  <%= f.text_area :content %>

  <%= f.submit "投稿する" %>
<% end %>